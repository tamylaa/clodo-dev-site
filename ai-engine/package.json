{
  "name": "ai-engine",
  "version": "1.0.0",
  "description": "Standalone AI Engine â€” multi-model LLM worker for SEO analytics capabilities",
  "type": "module",
  "main": "src/worker.mjs",
  "scripts": {
    "dev": "wrangler dev --config config/wrangler.toml",
    "deploy": "wrangler deploy --config config/wrangler.toml",
    "deploy:staging": "wrangler deploy --config config/wrangler.toml --env staging",
    "deploy:production": "wrangler deploy --config config/wrangler.toml --env production",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "tail": "wrangler tail --config config/wrangler.toml",
    "kv:list": "wrangler kv key list --config config/wrangler.toml --binding KV_AI",
    "setup:github-secrets": "node scripts/setup-github-secrets.mjs",
    "setup:secrets-interactive": "node scripts/setup-secrets-interactive.mjs",
    "post-deploy": "node scripts/post-deploy.mjs",
    "post-deploy:staging": "node scripts/post-deploy.mjs --staging",
    "post-deploy:local": "node scripts/post-deploy.mjs --local",
    "post-deploy:skip-wait": "node scripts/post-deploy.mjs --skip-workflow-wait"
  },
  "dependencies": {
    "compromise": "^14.14.5",
    "csv-parse": "^6.1.0",
    "csv-stringify": "^6.6.0",
    "uuid": "^13.0.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^4.0.18",
    "vitest": "^4.0.18",
    "wrangler": "^4.63.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "private": true
}
